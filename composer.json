{
  "name": "paytic/common-objects",
  "description": "Common value objects used in payments",
  "license": "MIT",
  "type": "library",
  "keywords": [],
  "authors": [
    {
      "name": "Gabriel Solomon",
      "email": "hello@gabrielsolomon.ro"
    }
  ],
  "homepage": "https://github.com/paytic/common-objects",
  "require": {
    "php": "^7.2 || ^8.0"
  },
  "require-dev": {
    "bytic/phpqatools": "^1.0"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "Paytic\\CommonObjects\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Paytic\\CommonObjects\\Tests\\Fixtures\\": "tests/fixtures",
      "Paytic\\CommonObjects\\Tests\\": "tests/src"
    }
  },
  "config": {
    "allow-plugins": {
      "phpstan/extension-installer": true,
      "ergebnis/composer-normalize": true,
      "dealerdirect/phpcodesniffer-composer-installer": true,
      "bytic/phpqatools": true
    }
  }
}
